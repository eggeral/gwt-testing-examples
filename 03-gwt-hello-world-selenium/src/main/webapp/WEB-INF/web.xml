<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

    <servlet>
        <servlet-name>echoServlet</servlet-name>
        <servlet-class>egger.software.gwt.server.EchoServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>echoServlet</servlet-name>
        <url-pattern>/gwt_hello_world_selenium/echo</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>gwt_hello_world_selenium.html</welcome-file>
    </welcome-file-list>

</web-app>
